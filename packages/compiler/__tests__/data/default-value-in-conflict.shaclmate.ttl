@prefix : <http://example.com/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

:NodeShape
    a rdfs:Class, sh:NodeShape ;
    sh:property [
        sh:defaultValue :defaultValue ;
        sh:in ( :otherValue );
        sh:maxCount 1 ;
        sh:path :path
    ] .
