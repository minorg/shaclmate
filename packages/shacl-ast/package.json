{
  "dependencies": {
    "@rdfjs/term-map": "^2.0.2",
    "@rdfjs/term-set": "^2.0.3",
    "@rdfjs/types": "^1.1.0",
    "@tpluscode/rdf-ns-builders": "^4.3.0",
    "@types/n3": "^1.26.0",
    "@types/rdfjs__term-map": "^2.0.10",
    "@types/rdfjs__term-set": "^2.0.9",
    "n3": "^1.26.0",
    "purify-ts": "^2.1.0",
    "rdfjs-resource": "1.0.24"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tsconfig/node18": "^18.2.4",
    "@tsconfig/strictest": "^2.0.5",
    "@types/node": "^22",
    "depcheck": "^1.4.7",
    "rimraf": "^6.0.1",
    "typescript": "5.8.2",
    "vitest": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4"
  },
  "files": [
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "name": "@shaclmate/shacl-ast",
  "packageManager": "npm@10.9.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minorg/shaclmate"
  },
  "scripts": {
    "build": "tsc -b",
    "build:noEmit": "tsc --noEmit",
    "check": "biome check",
    "check:write": "biome check --write",
    "check:write:unsafe": "biome check --write --unsafe",
    "clean": "rimraf dist",
    "depcheck": "depcheck .",
    "dev": "tsc -w --preserveWatchOutput",
    "dev:noEmit": "tsc --noEmit -w --preserveWatchOutput",
    "link-dependencies": "npm link rdfjs-resource",
    "test": "biome check && vitest run",
    "test:coverage": "biome check && vitest run --coverage",
    "test:watch": "biome check && vitest watch",
    "unlink": "npm unlink -g @shaclmate/shacl-ast"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "3.0.3"
}
