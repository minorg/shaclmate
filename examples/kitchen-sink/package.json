{
  "dependencies": {
    "@rdfjs/types": "~2.0.1",
    "@types/n3": "~1.26.0",
    "@types/sparqljs": "3.1.12",
    "@types/uuid": "~9.0.1",
    "js-sha256": "~0.11.0",
    "n3": "~1.26.0",
    "purify-ts": "~2.1.4",
    "rdfjs-resource": "2.0.0",
    "sparqljs": "3.7.3",
    "uuid": "~9.0.1",
    "zod": "~4.1.12"
  },
  "devDependencies": {
    "@kos-kit/sparql-client": "2.0.116",
    "@rdfjs/prefix-map": "~0.1.2",
    "@rdfjs/serializer-turtle": "~1.1.5",
    "@tpluscode/rdf-ns-builders": "~4.3.0",
    "@types/rdfjs__prefix-map": "~0.1.5",
    "@types/rdfjs__serializer-turtle": "~1.1.0",
    "oxigraph": "0.4.11",
    "rdf-validate-shacl": "0.5.8",
    "@biomejs/biome": "2.3.10",
    "@tsconfig/node18": "~18.2.4",
    "@tsconfig/strictest": "~2.0.8",
    "@types/node": "~22",
    "depcheck": "~1.4.7",
    "rimraf": "~6.0.1",
    "typescript": "5.9.3",
    "vitest": "~4.0.18",
    "@vitest/coverage-v8": "~4.0.18"
  },
  "files": [
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "name": "@shaclmate/kitchen-sink-example",
  "packageManager": "npm@10.9.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minorg/shaclmate"
  },
  "scripts": {
    "build": "tsc -b",
    "build:noEmit": "tsc --noEmit",
    "check": "biome check",
    "check:write": "biome check --write",
    "check:write:unsafe": "biome check --write --unsafe",
    "clean": "rimraf dist",
    "depcheck": "depcheck .",
    "dev": "tsc -w --preserveWatchOutput",
    "dev:noEmit": "tsc --noEmit -w --preserveWatchOutput",
    "link-dependencies": "npm link rdfjs-resource",
    "dev:tests": "tsc -p __tests__ -w --preserveWatchOutput",
    "test": "biome check && vitest run",
    "test:coverage": "biome check && vitest run --coverage",
    "test:watch": "biome check && vitest watch",
    "unlink": "npm unlink -g @shaclmate/kitchen-sink"
  },
  "type": "module",
  "types": "./dist/index.d.ts",
  "version": "3.0.4"
}
