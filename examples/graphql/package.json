{
  "dependencies": {
    "@shaclmate/runtime": "3.0.0",
    "graphql-yoga": "5.14.0"
  },
  "devDependencies": {},
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "license": "Apache-2.0",
